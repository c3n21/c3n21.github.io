---
import { getCVInformation } from "../utils/getCVInformation"

const cvInformation = getCVInformation()
---
<div>
	<img 
	src="/myphoto.jpg"
	/>
	<p>
		{
		cvInformation.About
		}
	</p>
</div>

<style>
	img { 
		/**
* Circular image
		*/
		width: 10rem;
		clip-path: circle(50% at 50% 50%);
	}


	div {
		display: flex;
		justify-content: center;
		align-items: center;
	}
</style>
